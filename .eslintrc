{
  "env": {
    "browser": true,
  },
  parser: "babel-eslint",
  "rules": {
    "jsx-a11y/href-no-hash": "off",
    "jsx-a11y/anchor-is-valid": ["warn", { "aspects": ["invalidHref"] }],
  },
  "extends": ["airbnb", "prettier"],
  "settings": {
    "import/resolver": {
      "webpack": {
        "config": "webpack.config.js",
      },
    },
  },
}
